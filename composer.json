{
    "name": "jaguarsoft/laravel-env-loader",
    "description": "Loads Laravel environment variables from Custom Service",
    "keywords": ["env", "dotenv", "laravel", "environment"],    
    "license": "MIT",
    "authors": [
        {
            "name": "Laurence",
            "email": "laurence@jaguarsoft.pe"
        }
    ],
    "autoload": {
        "psr-4": {
            "JaguarSoft\\LaravelEnvLoader\\": "src/"
        }
    },
    "require": {
        "php": "^7.0 || ^8.0",
        "vlucas/phpdotenv": "^3.3",
        "symfony/yaml": "^3.4"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.8.x-dev"
        },
        "laravel": {
            "providers": [                
                "JaguarSoft\\LaravelEnvLoader\\Provider\\AppBindProvider"
            ],
            "aliases": {
                "JaguarVar": "JaguarSoft\\LaravelEnvLoader\\Facade\\JaguarVar"
            }
        }
    }
}
